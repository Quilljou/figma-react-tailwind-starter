{
  "name": "figma-react-tailwind-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint --quiet src --ext .ts,.tsx && stylelint src/**/*.less",
    "lint:fix": "eslint --quiet --fix src --ext .ts,.tsx && stylelint --fix src/**/*.less",
    "dev": "concurrently -n plugin,next 'npm run build:plugin -- --watch --define:SITE_URL=\\\"http://localhost:3000\\\"' 'next dev'",
    "start": "next start",
    "build": "concurrently -n plugin,next 'npm run build:plugin -- --define:SITE_URL=\\\"$npm_package_config_siteURL\\\"' 'next build'",
    "build:plugin": "esbuild src/plugin/code.ts --bundle --target=es6 --loader:.svg=text --outfile=src/plugin/dist/code.js",
    "build:next": "next build"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^4.29.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "concurrently": "^8.2.0",
    "figx": "^0.1.2",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "ofetch": "^1.3.3",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-singlefile": "^0.13.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@figma/plugin-typings": "^1.82.0",
    "@tanstack/react-query-devtools": "^4.29.1",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.19.9",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-prettier": "3",
    "tailwindcss": "3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-external": "^1.2.8",
    "vite-plugin-svgr": "^2.4.0"
  },
  "config": {
    "siteURL": "https://yourapp.com"
  }
}
